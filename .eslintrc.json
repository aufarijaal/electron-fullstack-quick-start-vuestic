{
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "process": true,
    "__dirname": true
  },
  "extends": ["eslint:recommended", "plugin:vue/vue3-essential"],
  "overrides": [],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "semi": "error",
    "quotes": "error"
  }
}
